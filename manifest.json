{
  "manifest_version": 2,

  "name": "Subjective Tab Manager",
  "description": "Improved window and tab management for Chrome",
  "version": "0.7.1",

  "browser_action": {
    "default_icon": "images/glyphicons_154_more_windows.png",
    "default_popup": "popup.html",
    "default_title": "Subjective Tab Manager"
  },
  "permissions": [
    "tabs", "bookmarks", "contextMenus", "<all_urls>"
  ],
  "background": {
    "scripts": ["lib/jquery-1.10.2.js", "lib/jquery-ui-1.10.4.custom.min.js", "tabman.js"]
  },
  "icons": {
    "16": "images/glyphicons_154_more_windows.png",
    "48": "images/Multimedia-12-48.png",
    "128": "images/Multimedia-12-128.png"
  }
}
