{
  "manifest_version": 2,

  "name": "Subjective Tab Manager",
  "description": "A tab manager for organizing tabs in to windows by subject",
  "version": "0.1",

  "browser_action": {
    "default_icon": "images/glyphicons_154_more_windows.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "bookmarks", "<all_urls>"
  ],
  "background": {
    "scripts": ["lib/jquery-1.10.2.js", "lib/jquery-ui-1.10.4.custom.min.js", "tabman.js"]
  }
}
